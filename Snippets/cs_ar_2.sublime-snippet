    var col = -1;
    if (educacion === 4) {
        col = -1 + aportantes;
    } else if (educacion === 3) {
        col = 2 + aportantes;
    } else if (educacion === 2) {
        col = 5 + aportantes;
    } else if (educacion === 1) {
