        ];
        // Directivo
        if (Posicion === 1) {
            msNSE.setSelected(matriz[(-1 + (Empleados > 3 ? 3 : Empleados))][col]);
        } else if (Posicion === 2) {
            msNSE.setSelected(matriz[(2 + (Empleados * 2) + diff - 1)][col]);
        } else if (Posicion === 3) {            
